<Application x:Class="lsight.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" 
             xmlns:lsight="clr-namespace:lsight" xmlns:Converters="clr-namespace:lsight.Converters">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary>
                    <lsight:lsightBootstrapper x:Key="bootstrapper" />
                </ResourceDictionary>
            </ResourceDictionary.MergedDictionaries>
        	<Converters:TimestampConverter x:Key="TimestampConverter" />
        </ResourceDictionary>
    </Application.Resources>
</Application>
